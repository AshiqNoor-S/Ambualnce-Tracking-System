<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LifeTrack</title>

    <!-- Bootstrap CDN -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>

    <!-- Custom CSS -->
    <link rel="stylesheet" href="/styles/navbar.css">
    <link rel="stylesheet" href="/styles/style.css">
    <link rel="stylesheet" href="/styles/features.css">
    <link rel="stylesheet" href="/styles/team.css">
    <link rel="stylesheet" href="/styles/footer.css">


    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lora:wght@400;500;600;700&display=swap" rel="stylesheet">

    <script src="https://kit.fontawesome.com/8c4e0c6018.js" crossorigin="anonymous"></script>

    <script type="text/javascript">window.$crisp=[];
        window.CRISP_WEBSITE_ID="3b333d61-dba4-47fd-8b93-c0d45e7e0bdf";
        (function()
        {d=document;
        s=d.createElement("script");
        s.src="https://client.crisp.chat/l.js";
        s.async=1;d.getElementsByTagName("head")[0].appendChild(s);})();
    </script>

</head>
<body onload="typeWriter();">
    <%- include('navbar'); -%>
    <%- include('hero'); -%>


    <div class="wrap">
        <div class="box">
          <div class="box-top">
            <img class="box-image" src="/images/amb.png" alt="Ambulance">
            <div class="title-flex">
              <h3 class="box-title">Ambulance Tracking</h3>
              <p class="user-follow-info">24x7</p>
            </div>
            <p class="description">Lorem Ipsum Lorem ipsum</p>
          </div>
          <button class="button1 button"><a href="/map">Start Tracking</a></button>
        </div>
    
        <div class="box">
          <div class="box-top">
            <img class="box-image" src="/images/vt.png" alt="Health Vitals">
            <div class="title-flex">
              <h3 class="box-title">Tracking Health Vitals</h3>
              <p class="user-follow-info">24x7</p>
            </div>
            <p class="description">Lorem Ipsum Lorem ipsum</p>
          </div>
          <button class="button1 button" >Track Patient</button>
        </div>
    
        <div class="box">
          <div class="box-top">
            <img class="box-image" src="/images/ht.png" alt="Doctor">
            <div class="title-flex">
              <h3 class="box-title">Health Tips</h3>
              <p class="user-follow-info">24x7</p>
            </div>
            <p class="description">Lorem Ipsum Lorem ipsum</p>
          </div>
          <button class="button1 button" >Read</button>
        </div>
    
        <div class="box">
            <div class="box-top">
              <img class="box-image" src="/images/cc.png" alt="Doctor">
              <div class="title-flex">
                <h3 class="box-title">Live Chat</h3>
                <p class="user-follow-info">24x7</p>
              </div>
              <p class="description">Lorem Ipsum Lorem ipsum</p>
            </div>
            <button class="button1 button" >Start Chatting</button>
          </div>
        
      </div>





    <%- include('team'); -%>
    <%- include('footer'); -%>
    <script type="text/javascript">
        $crisp=[];
        $crisp.push(["set", "user:nickname", ["visitor_nickname"]]);
        $crisp.push(["set", "session:data", [["custom_key", "custom_value"]]]);
        $crisp.push(["set", "session:event", ["trigger", "chat_opened"]]);
    </script>

    <script src="script.js"></script>
    
</body>
</html>